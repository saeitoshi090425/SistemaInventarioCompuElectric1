<UserControl x:Class="SistemaInventarioCompuElectric1.HISTORIAL.historial_ventana_principal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SistemaInventarioCompuElectric1.HISTORIAL"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White" Margin="40">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />

            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <TextBlock Text="Historial" 
               Foreground="#143c5b"
               FontWeight="Bold"
               FontSize="31"
               Margin="0,0,0,30"
               />
        <TextBlock Text="A continuación, se visualizan el historial de los movimientos en los productos." 
        Foreground="#143c5b"
        FontWeight="Normal"
        FontSize="18"
        Margin="0,55,0,30"
        />



    </Grid>



</UserControl>
